[user]
    name = ning
    email = ningyuan.sg@gmail.com
[alias]
    logp = log --pretty=\"%Cred%h @ %cD - %Cgreen%cn%Creset : %s\"
    graph = git log --graph --oneline --decorate
    lf = ls-files
    cv = commit -v
    nuke = reset --hard
    assume-unchanged = update-index --assume-unchanged
[core]
    editor = "vim"
[push]
    default = current
